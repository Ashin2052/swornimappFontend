    "@fortawesome/fontawesome-free": "~5.6.3",
    "@ng-toolkit/universal": "1.1.47",
    "@nguniversal/express-engine": "8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@types/chart.js": "^2.7.40",
    "angular-bootstrap-md": "^8.10.1",
